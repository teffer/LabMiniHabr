# https://aka.ms/editorconfigdocs
root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = tab
insert_final_newline = true
trim_trailing_whitespace = true

[*.dbs]
insert_final_newline = false

[*.sln]
charset = utf-8-bom
end_of_line = crlf

